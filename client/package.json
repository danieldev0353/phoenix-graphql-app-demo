{
  "name": "yummy",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link": "^1.0.0",
    "apollo-link-context": "^1.0.0",
    "apollo-link-error": "^1.0.0",
    "apollo-link-http": "^1.1.0",
    "bulma": "0.6.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "moment": "^2.19.1",
    "react": "16.0.0",
    "react-apollo": "2.0.0",
    "react-dom": "16.0.0",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.16",
    "redux": "^3.7.2",
    "redux-form": "7.1.2",
    "shortid": "^2.2.8"
  },
  "scripts": {
    "build-css": "node-sass --include-path node_modules/ --include-path src/ src/assets/stylesheets/scss -o src/assets/stylesheets/css",
    "watch-css": "npm run build-css && node-sass --include-path node_modules/ --include-path src/ src/assets/stylesheets/scss -o src/assets/stylesheets/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-prettier": "2.7.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "prettier": "1.7.4"
  }
}
